[global]
direct=1
numjobs=6

ioengine=psync
iodepth=1
allow_mounted_write=1
time_based=1
group_reporting=1

bs=4k
size=10G
runtime=30
rw=read

[file1]
filename=/dev/sdb

[file2]
filename=/dev/sdc

[file3]
filename=/dev/sdd

[file4]
filename=/dev/sde

[file5]
filename=/dev/sdf

[file6]
filename=/dev/sdg

[file7]
filename=/dev/sdh

[file8]
filename=/dev/sdi
